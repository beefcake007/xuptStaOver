<script type="text/javascript">
	function checkForm(){
		var name=$("#name").val();
		var email=$("#email").val();
		var mobile=$("#mobile").val();
		var professional=$("#professional").val();
		var message=$("#message").val();
		if(name==""){
			alert("姓名不能为空");
			return false;
		}
		if(email==""){
			alert("邮箱不能为空");
			return false;
		}
		if(mobile==""){
			alert("手机号不能为空");
			return false;
		}
		if(professional==""){
			alert("专业班级不能为空");
			return false;
		}
		if(message==""){
			alert("留言不能为空");
			return false;
		}
		return true;
	}
